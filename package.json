{
  "name": "webrecorderplayer-electron",
  "version": "0.5.0",
  "description": "webrecorderplayer - standalone warc viewer",
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webrecorder/webrecorderplayer-electron.git"
  },
  "keywords": [
    "warc",
    "webarchives",
    "electron",
    "pywb",
    "webrecorder"
  ],
  "author": "webrecorder/rhizome",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/webrecorder/webrecorderplayer-electron/issues"
  },
  "homepage": "https://github.com/webrecorder/webrecorderplayer-electron#readme",
  "build": {
    "asar": false,
    "appId": "org.webrecorder.webrecorderplayer",
    "mac": {
      "category": "public.app-category.utilities",
      "publish": {
        "provider": "s3",
        "bucket": "webrecorder-builds",
        // "path": "/electron"
      }
    },
    "linux": {
      "publish": {
        "provider": "s3",
        "bucket": "webrecorder-builds",
        // "path": "/electron"
      }
    }
  },
  "dependencies": {
    "portfinder": "^1.0.10",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "electron": "1.6.2",
    "electron-builder": "15.6.2",
    "electron-publisher-s3": "^16.4.0"
  }
}
